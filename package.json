{
  "name": "satracker-native",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "desktop": "pnpm build:web && (node api/index.js & cd electron && pnpm exec electron main.js)",
    "build:web": "expo export -p web --output-dir electron/dist && node fix-electron-paths.js",
    "build:android": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios",
    "build:macos": "pnpm build:web && cd electron && pnpm run build:mac",
    "build:windows": "pnpm build:web && cd electron && pnpm run build:win",
    "build:desktop": "pnpm build:web && cd electron && pnpm run build:all",
    "server": "node api/index.js",
    "dev:api": "pnpm build:web && pnpm server"
  },
  "homepage": "./",
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@google/generative-ai": "^0.24.1",
    "@openrouter/sdk": "^0.5.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/drawer": "^7.7.13",
    "@supabase/supabase-js": "^2.93.3",
    "cors": "^2.8.6",
    "expo": "~54.0.32",
    "expo-av": "~16.0.8",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "^18.0.13",
    "expo-font": "^14.0.11",
    "expo-linking": "^8.0.11",
    "expo-notifications": "^0.32.16",
    "expo-router": "^6.0.22",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "lucide-react-native": "^0.562.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.20.0",
    "react-native-svg": "^15.15.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "resend": "^4.8.0",
    "zustand": "^5.0.10"
  },
  "private": true,
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-native": "^0.73.0",
    "babel-preset-expo": "~54.0.10",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3"
  }
}
